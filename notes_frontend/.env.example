# Frontend environment variables

# API base URL for the backend (preferred)
REACT_APP_API_BASE=http://localhost:4000/api

# Alternative name supported (if used elsewhere)
REACT_APP_BACKEND_URL=http://localhost:4000/api

# Optional runtime variables (not used directly by this app, but listed for platform consistency)
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
